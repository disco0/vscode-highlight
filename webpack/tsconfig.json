{
    "files":
    [
        "webpack.config.ts"
    ],
    "include": [],
    "exclude":
    [
      "../src",
      "../dist",
      "../node_modules"
    ],
    "compilerOptions":
    {
      "typeRoots": ["../node_modules/@types"],
      "moduleResolution": "Node",
      "skipLibCheck": true,
      "types": ["node", "webpack"],

      "module": "commonjs",
      "target": "es5",

      "checkJs": false,

      "esModuleInterop": true,

      "resolveJsonModule": true,

      // Disable compilation
      "noEmit": true,
    }
}
